<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.app.mapper.CoursesLabelCourseMapper">

	<delete id="deleteByMainId" parameterType="java.lang.String">
		DELETE 
		FROM  courses_label_course 
		WHERE
			 course_id = #{mainId} 	</delete>
	
	<select id="selectByMainId" parameterType="java.lang.String" resultType="org.jeecg.modules.app.entity.CoursesLabelCourse">
		SELECT * 
		FROM  courses_label_course
		WHERE
			 course_id = #{mainId} 	</select>
</mapper>
